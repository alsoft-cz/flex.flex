class private c_rtl_memchain =
    ------------------------------------------------------------------------------------------------
    override free(hdr : in p_rtl_chain_item);
    -- vola se pri clear all, tato implementace uvolnuje pamet
    ------------------------------------------------------------------------------------------------
    begin
      first^.dealocator^.memmgr_free(num,hdr2adr(p_rtl_memmgr_header(hdr:unchecked)),^this);
      end free;
    end c_rtl_memchain;

--end memchains;