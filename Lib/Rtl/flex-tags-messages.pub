----- Message descriptor ---------------------------------------------------------------------------
type
  t_desc_message   = t_desc_typed with record
      end record
          for alignment use 1
#if #environment compiler_version;='4.0.3.8' then;
          for size      use t_desc_typed:size;
#else;
          for size      use 16;
#end if;
  p_desc_message   = ^t_desc_message for machine_pointer use true;

end messages;