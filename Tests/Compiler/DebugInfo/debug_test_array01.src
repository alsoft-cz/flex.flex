program debug_array_test01 =

with
  standard;

type
  test_type1 = enum
    v1;
    v2;
    v3;
    v4;
    end enum;

  test_type2 = enum
    i1;
    i2;
    i3;
    i4;
    i5;
    i6;
    end enum;
  
  test_type3 = array test_type2 of test_type1;
  test_type4 = array of test_type1;


var
  test_array1      : test_type3 := [        for i3 use v2,
                                            for i5 use v4,
                                            for i1 use v1,
                                            for others use v3
                                   ];
  var1             : test_type1;
  var2             : test_type1;


procedure foo (
    par1_array     : in test_type3) =
begin
  var1:=par1_array[i2];
  var2:=par1_array[i4];
  end foo;

procedure bar (
    _par1_array    : in out test_type3) =
begin
  _par1_array[i2]:=v4;
  _par1_array[i4]:=v4;
  end bar;

begin
  var1:=test_array1[i3];
  var2:=test_array1[i6];
  test_array1[i3]:=var2;
  test_array1[i6]:=var1;
  foo(test_array1);
  bar(test_array1);
  end debug_array_test01;
