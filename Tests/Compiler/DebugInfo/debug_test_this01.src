program debug_test_this01 =

with
  standard;


type
  test_enum = enum
    v1;
    v2;
    v3;
    v4;
    end enum;

  test_record = record
    value1         : test_enum;
    value2         : test_enum;
    end record;



class public test_class0 =
  end test_class0;

class private test_class0 =
  var
    data0          : character 8 := "0";

  end test_class0;


class public test_class = extend test_class0;
  var
    data1          : t_unsigned := 1;
    data2          : t_unsigned := 2;
    data3          : t_unsigned := 3;

    static method1 (
        p1         : in t_unsigned);

    static method2 (
        p2         : in out t_unsigned);

  end test_class;


class private test_class =
  var 
    dataREC        : test_record := [for value1 use v1, for value2 use v2];

    static method1 =
    begin
      data1:=p1;
      end method1;

    static method2 =
    begin
      this.data2:=p2;
      p2:=this.data3;
      end method2;

  end test_class;



var
  a                : t_unsigned := 12;
  b                : t_unsigned := 4;
  cl1              : test_class;

begin
  cl1.data2:=78;
  cl1.data3:=6;
  cl1.data1:=a div b;
  cl1.method1(15);
  cl1.method2(a);

  end debug_test_this01;
