----------------------------------------------------------------------------------------------------
program calling1 =
----------------------------------------------------------------------------------------------------
type
  t_pointer = ^unchecked for machine_pointer use true;
  t_signed  = signed 32;

  t_mystruct = record
    a : t_pointer;
    b : t_signed;
    end record;

  t_my_cdecl_call = procedure (
    pata           : in t_pointer;
    data           : in t_pointer
  );
  --#pragma convention(t_my_cdecl_call,cdecl);


----------------------------------------------------------------------------------------------------
procedure my_cdecl: t_my_cdecl_call = 
#pragma convention(my_cdecl,cdecl);
----------------------------------------------------------------------------------------------------
begin
  
  end my_cdecl;


var
   my : aliased t_mystruct;

begin
  my.b:=123456;

  my_cdecl(^my,^my);
{
  #syntax asm;
    lea  eax,my;
    push eax;
    push eax;
    call my_cdecl;
    pop  eax;
    pop  eax;
    #end asm;
}
  end calling1;