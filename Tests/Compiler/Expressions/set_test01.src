program set_test01 =

with
  standard,
  standard.console,
  standard.conversions;

type
  t_unum = unsigned 32;
  t_base = t_unum range 1..1567;
  t_set = set of t_base;

var
  s1  : t_set;
  s2  : t_set;
  s3  : t_set;
  s4  : t_set;

procedure dump_set (
    s : t_set) =
begin
  write('[');
  for i in t_base:first..t_base:last loop
    if i in s then
      write(unsigned_to_string(i:ord));
      write(' ');
      end if;
    end loop;
  write_line(\8']');
  end dump_set;

procedure create_set return t_set =
begin
  result:=s2;
  end create_set;

procedure go_test =
#option dump_code(true);
begin
  s4 + s2 + s3;
  end go_test;

begin
  s1 := [t_base(2),t_base(3),t_base(7),t_base(8),t_base(t_base:last:ord-2)];
  s2 := [t_base(2),t_base(5),t_base:last];
  s3 := [t_base(1),t_base(7),t_base(9),t_base(t_base:last:ord-1)];
  s4 := s1;
  go_test;
  dump_set(s1);
  dump_set(s2);
  dump_set(s3);
  write_line('==');
  dump_set(s4);
  end set_test01;
