procedure entity_set_position =
-- Nastav¡ pozici zadan‚ entity.
----------------------------------------------------------------------------------------------------
begin
  for x in tentityposition:first..level loop 
    if entity.pos[x].b.num=0 then 
      entity.pos[x]:=pos;
      end if;
    end loop; 
  end entity_set_position;



